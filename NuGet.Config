<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
    <!-- Optional: add your private feed(s) below -->
    <!-- <add key="MyPrivateFeed" value="https://myfeed.local/nuget" /> -->
  </packageSources>

  <!-- Optional: prefer nuget.org when multiple sources contain same packages -->
  <activePackageSource>
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
  </activePackageSource>

  <!-- Optional: configure package restore behavior -->
  <config>
    <add key="dependencyBehavior" value="Lowest" />
  </config>
</configuration>
